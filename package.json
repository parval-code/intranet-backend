{
  "name": "intranet-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/src/server.js",
    "start:staging": "npm run build && cross-env NODE_ENV=staging node dist/src/server.js",
    "start:production": "npm run build && cross-env NODE_ENV=production node dist/src/server.js",
    "start:build": "npm build && npm run start NODE_OPTIONS=--max-http-header-size=200000",
    "start:dev": "cross-env NODE_OPTIONS=--max-http-header-size=200000 CIPHER_KEY=398e8d90e4363cb72aebccc4242351e9a4e26cb7285d6015c578d5b14d2f324b TZ=UTC nodemon dist/src/server.js",
    "build:watch": "npm run build && npm-run-all -p watch start:dev",
    "build": "./node_modules/.bin/tsc",
    "watch": "tsc -w",
    "test": "cross-env NODE_ENV=test jest --runInBand --bail",
    "coverage": "node_modules/.bin/cross-env NODE_ENV=test nyc mocha",
    "migration:seed": "node_modules/.bin/ts-node data/seeders/cli/Seed.ts",
    "migration:drop": "node_modules/.bin/ts-node ./node_modules/typeorm/cli.js schema:drop",
    "migration:create": "node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "nyc": {
    "lines": 60,
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@azure/storage-blob": "^12.13.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/faker": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.136",
    "@types/node-cron": "^2.0.2",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@types/supertest": "^2.0.8",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cloudinary": "^1.35.0",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "crypto-js": "^4.1.1",
    "debug": "^3.1.0",
    "dotenv": "^16.0.3",
    "express": "~4.15.5",
    "express-fileupload": "^1.4.0",
    "faker": "^4.1.0",
    "glob": "7.1.1",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mssql": "^7.2.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysqli": "^3.1.4",
    "nock": "^11.3.3",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.9.3",
    "nyc": "^14.1.1",
    "pg": "^8.9.0",
    "redis": "3",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.18",
    "uuid": "^9.0.0",
    "xml2js": "^0.6.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/compression": "^0.0.35",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.0.39",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.49",
    "eslint-config-airbnb": "^17.1.1",
    "jest": "^21.2.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.1",
    "nsp": "^3.1.0",
    "prettier": "^1.7.4",
    "ts-jest": "^21.1.4",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-immutable": "^4.4.0",
    "typescript": "4.8.4"
  }
}
